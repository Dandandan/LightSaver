Meteor.startup(function(){
Meteor.setInterval( function(){
  Energy.insert({
 	 createdBy: "jacco",
 	 createdAt: new Date(),
	  title: "My first post!",
 	 content: "Today was a good day."
 	});

  livedata = HTTP.get("https://live.mpare.net/live.json?user=1").data;

 Energy.update({
  title: {$regex: /first/}
     }, {
  $set: {content: livedata}
	});
	} 
,1000)

});


